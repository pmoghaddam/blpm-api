{
    "node": true,
    "browser": true,
    "esnext": true,
    "bitwise": false,
    "camelcase": false,
    "curly": false,
    "eqeqeq": true,
    "immed": true,
    "latedef": true,
    "newcap": true,
    "noarg": true,
    "regexp": true,
    "undef": true,
    "unused": true,
    "strict": true,
    "trailing": true,
    "smarttabs": false,
    "predef": [
        "rekuire",
        "define",
        "require",
        "exports",
        "module",
        "expect",
        "assert",
        "describe",
        "before",
        "beforeEach",
        "after",
        "afterEach",
        "it"
    ],
    "indent": 4,
    "maxlen": 120,
    "devel": false,
    "noempty": true
}